# Apple Store Connect API Configuration
APPLE_KEY_ID=your_key_id_here
APPLE_ISSUER_ID=your_issuer_id_here
APPLE_BUNDLE_ID=com.yourcompany.yourapp
APPLE_APP_STORE_ID=your_app_store_id

# Apple Store Connect API Private Key (keep this secure!)
APPLE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----
your_private_key_content_here
-----END PRIVATE KEY-----"

# Stytch OAuth Configuration (for Claude iOS support)
STYTCH_PROJECT_ID=project-test-uuid-here
STYTCH_PROJECT_DOMAIN=https://test.stytch.com
STYTCH_SECRET=secret-test-key-here
STYTCH_PUBLIC_TOKEN=public-token-test-here
STYTCH_JWKS_URI=https://test.stytch.com/.well-known/jwks.json

# Server Configuration
PORT=3001
HOST=0.0.0.0
CORS_ORIGIN=*

# OAuth Settings
OAUTH_ENABLED=false
# Set OAUTH_ENABLED=true to enable OAuth authentication for Claude iOS support
# When enabled, configure Stytch settings above